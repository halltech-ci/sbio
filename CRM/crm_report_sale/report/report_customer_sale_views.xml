    <template id="report_pos_custmer_sale_by_lot">
        <t t-call="web.html_container">
                <div class="article mt0 o_report_layout_standard">
                    <div class="container">
                        <div class="row">
                            <div class="col-sm" style="display: block;text-align:center;">
                                <h3 class="text-center">RAPPORT LISTE CLIENT
                                    <span style="font-size: 14px;"><strong>De</strong>: <t t-esc="date_start"/> <strong>A</strong>: <t t-esc="date_end"/></span>
                                </h3>
                                <br/>
                           
                            </div>
                        </div>
                    </div>
                    <br/><br/>
                    <table class="table table-sm table-condensed table-bordered" >
                        <thead class="thead-dark">
                            <tr>
                                <th class='text-center'>Date</th>
                                <th class='text-center'>Client</th>
                                <th class='text-center'>Téléphone</th>
                                <th class='text-center'>Article</th>
                                <th class='text-center'>Numero Lot</th>
                            
                            </tr>
                        </thead>
                        <tbody>
                            <t t-foreach="docs" t-as="lines"> 
                                    <t t-if="lines">
                                            <tr>
                                                <td> <span t-esc="lines['date']" /> </td>
                                                <td> <span t-esc="lines['nom_client']" /> </td>
                                                <td><span t-esc="lines['contact']" /> </td>
                                                <td><span t-esc="lines['article']" /> </td>
                                                <td><span t-esc="lines['lot']" /> </td>
                                                
                                            </tr>   
                                    
                                        </t>     
                            </t>

                        </tbody>
                    </table>
                    <br/>
            </div> 
            <div class="footer o_standard_footer" style="border-top: 1px solid black;">
                <div class="row text-center">
                    <div class="col col-12 text-center">
                        <ul class="list-inline">
                            <li class="list-inline-item">Page:</li>
                            <li class="list-inline-item">
                                <span class="page"/>
                            </li>
                            <li class="list-inline-item">/</li>
                            <li class="list-inline-item">
                                <span class="topage"/>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </t>
    </template>

</odoo>