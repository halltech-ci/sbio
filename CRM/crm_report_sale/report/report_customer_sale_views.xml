<odoo>
    <record id="paperformat_lot_report_sale" model="report.paperformat">
            <field name="name">POS</field>
            <field name="default" eval="False"/>
            <field name="format">custom</field>
            <field name="page_height">210</field>
            <field name="page_width">300</field>
            <field name="orientation">Portrait</field>
            <field name="margin_top">4</field>
            <field name="margin_bottom">9</field>
            <field name="margin_right">3</field>
            <field name="margin_left">3</field>
            <field name="header_line" eval="False"/>
            <field name="header_spacing">3</field>
            <field name="dpi">90</field>
    </record>

     <record id="report_pos_lot_sale_pdf" model="ir.actions.report">
            <field name="name">Rapport Client</field>
            <field name="model">report.number.lot.wizard</field>
            <field name="report_type">qweb-html</field>
            <field name="report_name">crm_report_sale.report_pos_custmer_sale_by_lot</field>
            <field name="report_file">crm_report_sale.report_pos_custmer_sale_by_lot</field>
    
            <field name="print_report_name">Rapport Client</field>
            <field name="binding_model_id" ref="model_report_number_lot_wizard"/>
            <field name="binding_type">report</field>
        </record>
    
        <record id="pos_report_generate_xlsx_report_by_lot" model="ir.actions.report">
            <field name="name">Rapport Client</field>
            <field name="model">report.number.lot.wizard</field>
            <field name="report_type">xlsx</field>
            <field name="report_name">crm_report_sale.customer_sale_number_lot_xlsx_generate</field>
            <field name="report_file">crm_report_sale.customer_sale_number_lot_xlsx_generate</field>
        </record>
