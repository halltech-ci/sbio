<odoo>
  <data>
      <record model="ir.ui.view" id="stock_inherit_lot_wizard">
      <field name="model">stock.production.lot</field>
      <field name="inherit_id" ref="stock.view_production_lot_form"/>
      <field name="arch" type="xml">
        <xpath expr="//sheet" position="before">
            <header>
                <button string="Printer" type="object" name="button_barcode_wizard" class="btn-primary" />
            </header>
          </xpath>
      </field>
    </record> 
  </data>
</odoo>