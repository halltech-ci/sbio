<odoo>
    <data>
        <template id="inherit_report_barcode_simple_label4x12" inherit_id="product.report_simple_label4x12">
             <xpath expr="//div[hasclass('o_label_name')] " position="replace">
                <p>
                </p>
            </xpath> 
            <xpath expr="//div[hasclass('o_label_left_column')] " position="replace">
                <p>
                </p>
            </xpath> 
            <xpath expr="//div[hasclass('o_label_price_medium')] " position="replace">
                <p>
                </p>
            </xpath>
            
            <xpath expr="//div[hasclass('o_label_left_column')] " position="replace">
