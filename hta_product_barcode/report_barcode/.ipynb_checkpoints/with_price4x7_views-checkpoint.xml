<odoo>
    <data>
        <template id="inherit_report_barcodesimple_label4x7" inherit_id="product.report_simple_label4x7">
            <xpath expr="//div[hasclass('o_label_name')] " position="replace">
                <p>
                </p>
            </xpath>
            <xpath expr="//div[hasclass('text-right')] " position="replace">
                <p>
                </p>
            </xpath>
            <xpath expr="//div[hasclass('text-center o_label_small_barcode')] " position="replace">
               <div class="text-center o_label_small_barcodes">
                    <t t-if="barcode">
                        <div t-out="barcode" style="padding:0" t-options="{'widget': 'barcode', 'quiet': 0, 'symbology': 'auto', 'img_style': barcode_size}"/>
                        <div class="o_label_name" style="line-height: 130%;height:2.7em;background-color: transparent;">
                            <span t-out="barcode"/>
                        </div>
                    </t>
                </div>
            </xpath>
            <xpath expr="//div[hasclass('text-center o_label_small_barcodes')]" position="after">
               <div class="container" style="font-size:11px;">
                  <div class="row">
                    <div class="col-6">
                      Fab:47/54/5555 <!--span class="text-nowrap" t-field="product.default_code"/-->
                    </div>
                    <div class="col-6">
                      Exp:43/55/6666 <!--span class="text-nowrap" t-field="product.default_code"/-->
                    </div>
                 </div>
                </div>
            </xpath>
        </template>
    </data>
</odoo>