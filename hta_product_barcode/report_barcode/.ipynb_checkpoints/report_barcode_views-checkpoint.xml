<odoo>
    <data>
        <template id="inherit_report_barcode" inherit_id="product.report_simple_label_dymo">
            <xpath expr="//div[hasclass('o_label_name')] " position="replace">
                <div class="o_label_names" style="line-height: 130%;height:2.7em;background-color: transparent;">
                    <span t-out="barcode"/>
                </div>
            </xpath>
             <xpath expr="//div[hasclass('o_label_name')] " position="replace">
                <p>
                </p>
            </xpath>
            <xpath expr="//div[hasclass('o_label_left_column')] " position="replace">
                <p>
                </p>
            </xpath>
             <xpath expr="//div[hasclass('text-right')] " position="replace">
                <div class="container" style="font-size:13px;">
                  <div class="row">
                    <div class="col-6">
                      Fab:47/54/5555 <!--span class="text-nowrap" t-field="product.default_code"/-->
                    </div>
                    <div class="col-6">
                      Exp:43/55/6666 <!--span class="text-nowrap" t-field="product.default_code"/-->
                    </div>
                 </div>
                </div>
            </xpath>
        </template>
    </data>
</odoo>